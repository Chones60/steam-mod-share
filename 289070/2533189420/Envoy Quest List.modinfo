<?xml version="1.0" encoding="utf-8"?>
<Mod id="389034b7-460b-40c1-be0c-a3240ebeff7b" version="1">
  <Properties>
    <Name>Envoy Quest List</Name>
    <Description>Adds a screen that organizes and lists available City-State quests.</Description>
    <Created>1642726058</Created>
    <Teaser>Organizes City-State Quests</Teaser>
    <Authors>Stack Man</Authors>
    <AffectsSavedGames>0</AffectsSavedGames>
    <CompatibleVersions>1.2,2.0</CompatibleVersions>
  </Properties>
  <FrontEndActions>
    <UpdateDatabase id="eql_settings">
      <File>config/eql_config.xml</File>
    </UpdateDatabase>
    <UpdateText id="eql_options_text">
      <File priority="1">config/en_us/eql_options_text.xml</File>
      <File>config/en_us/eql_translations_options_text.xml</File>
    </UpdateText>
  </FrontEndActions>
  <InGameActions>
    <AddUserInterfaces id="eql_questpopup">
      <Properties>
        <Context>InGame</Context>
      </Properties>
      <File>ui/eql_launchbutton.xml</File>
      <File>ui/eql_questpopup.xml</File>
    </AddUserInterfaces>
    <UpdateIcons id="eql_icons">
      <File>eql_icons.xml</File>
    </UpdateIcons>
    <ImportFiles id="eql_icon">
      <File>eql_launchbutton36.dds</File>
      <File>traderoute.dds</File>
    </ImportFiles>
    <UpdateText id="eql_text">
      <File priority="1">text/eql_text.xml</File>
      <File priority="1">config/en_us/eql_options_text.xml</File>
      <File>text/eql_translations_text.xml</File>
    </UpdateText>
  </InGameActions>
  <Files>
    <File>config/en_us/eql_options_text.xml</File>
    <File>config/en_us/eql_translations_options_text.xml</File>
    <File>config/eql_config.xml</File>
    <File>eql_icons.xml</File>
    <File>eql_launchbutton36.dds</File>
    <File>text/eql_text.xml</File>
    <File>text/eql_translations_text.xml</File>
    <File>traderoute.dds</File>
    <File>ui/eql_launchbutton.lua</File>
    <File>ui/eql_launchbutton.xml</File>
    <File>ui/eql_questpopup.lua</File>
    <File>ui/eql_questpopup.xml</File>
  </Files>
</Mod>